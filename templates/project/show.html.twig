{% extends 'layout/base.html.twig' %}

{% block main %}
    <h1>{{ project.name }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>Client</th>
                <td>{{ project.client }}</td>
            </tr>
            <tr>
                <th>projectDeveloper</th>
                <td>
                    {% for developer in projectDeveloper %}
                        {{ developer }}
                    {% endfor %}     
                </td>
            </tr>

        </tbody>
    </table>

{% endblock %}
